<% # loop through all filesystems needing export and servers needing access %>
<% @nfs_server[@env].each_pair do |path, servers| %>
<% servers.each_with_index do |server| %>
<%= "#{path}    #{server}" %>(rw,sync,no_root_squash,no_subtree_check)
<% end %>
<% end %>
